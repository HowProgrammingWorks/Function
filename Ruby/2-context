cities = ['Athens', 'Roma', 'London', 'Beijing', 'Kiev', 'Riga'];

f = -> (s) { s.length }

def my_map(arr, fn)
  result = []
  arr.each { |element| result << fn.call(element) }
  result
end

def f1()
  cities = ["Athens", "Roma"];
  f = -> (s) { s.upcase }
  puts cities
  puts my_map(cities,f)
end
